{
  "name": "business-logic-docs",
  "version": "1.0.0",
  "description": "Automated business logic documentation extraction using tree-sitter",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "extract": "tsx src/index.ts extract",
    "webhook": "tsx src/webhook/handler.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests"
  },
  "dependencies": {
    "@sourcegraph/amp-sdk": "latest",
    "tree-sitter": "^0.21.1",
    "tree-sitter-python": "^0.21.0",
    "web-tree-sitter": "^0.22.6",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "marked": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
